{
  "name": "zyntel-backend",
  "version": "1.0.0",
  "description": "Zyntel Dashboard Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "node -r ts-node/register migrations/run.ts",
    "ingest": "ts-node scripts/data-processing/ingest.ts",
    "transform": "ts-node scripts/data-processing/transform.ts",
    "export-meta": "ts-node scripts/maintenance/export-meta-csv.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "socket.io": "^4.6.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "csv-parse": "^5.5.3",
    "node-cron": "^3.0.3",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  }
}